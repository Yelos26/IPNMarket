<!-- BODY -->
<body class="body">

    <!-- HEADER -->
    <header class="header">

        <div class="logo-header" onclick="logo()">
            <img src="~/Img/logo/IPN Market.png" alt="IPNMarket Logo" class="logo">
            <h5>IPN Market</h5>
        </div>

        <script>
            function logo() {
                window.location.href = '@Url.Action("Home", "Home")';
            }
        </script>

        <div class="Buscador-container">
            <div class="Buscador">
                <div class="Buscador-icon-container">
                    <svg xmlns="http://www.w3.org/2000/svg" height="32" id="icon" viewBox="0 0 32 32" class="Buscador-icon">
                        <defs>
                            <style>
                                .cls-1 {
                                    fill: none;
                                }
                            </style>
                        </defs>
                        <title></title>
                        <rect height="2" width="14" x="14" y="25" />
                        <polygon points="7.17 26 4.59 28.58 6 30 10 26 6 22 4.58 23.41 7.17 26" />
                        <rect height="2" width="14" x="14" y="15" />
                        <polygon points="7.17 16 4.59 18.58 6 20 10 16 6 12 4.58 13.41 7.17 16" />
                        <rect height="2" width="14" x="14" y="5" />
                        <polygon points="7.17 6 4.59 8.58 6 10 10 6 6 2 4.58 3.41 7.17 6" />
                        <rect class="cls-1" data-name="&lt;Transparent Rectangle&gt;" height="32" id="_Transparent_Rectangle_" width="32" />
                    </svg>
                </div>
                <input id="buscador-input" class="Buscador-input" type="search" placeholder="Buscar..." />
            </div>
        </div>

        <script>
            document.getElementById('buscador-input').addEventListener('keypress', function (e) {
                if (e.key === 'Enter') {
                    e.preventDefault(); // Previene que el formulario se envíe automáticamente
