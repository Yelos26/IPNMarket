    <!-- CARRUSEL -->
    @{
        var carrusel = ViewBag.Carrusel as List<CarruselModel>;
    }

    @if (carrusel != null && carrusel.Any())
    {
        <div class="carrusel">
            <br />
            <br />
            <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
                <ol class="carousel-indicators">
                    @for (int i = 0; i < carrusel.Count; i++)
                    {
                        <li data-target="#carouselExampleIndicators" data-slide-to="@i" class="@(i == 0 ? "active" : "")"></li>
                    }
                </ol>
                <div class="carousel-inner">
                    @{
                        int index = 0;
                    }
                    @foreach (var carru in carrusel)
                    {
                        string imagen = "";
                        if (carru.Imagen != null && carru.Imagen.Length > 0)
                        {
                            string Base64 = Convert.ToBase64String(carru.Imagen);
                            string Extension = "jpg"; // Extensión predeterminada

                            // Determinar la extensión de la imagen
                            var header = carru.Imagen.AsEnumerable().Take(4).ToArray();
                            if (header.SequenceEqual(new byte[] { 137, 80, 78, 71 }))
                            {
                                Extension = "png";
                            }
                            else if (carru.Imagen.AsEnumerable().Take(2).SequenceEqual(new byte[] { 255, 216 }))
                            {
                                Extension = "jpeg";
                            }

                            // Crear el esquema data URI
                            imagen = $"data:image/{Extension};base64,{Base64}";
                        }

                        <div class="carousel-item @(index == 0 ? "active" : "")">
                            <center>
                                @if (!string.IsNullOrEmpty(imagen))
                                {
                                    if (carru.Accion == "Academica")
                                    {
                                        <img class="carrusel-img" src="@imagen" alt="@carru.ID_Carrusel" width="60" height="60" onclick="Academica()">
                                    }
                                    else if (carru.Accion == "Salud")
                                    {
                                        <img class="carrusel-img" src="@imagen" alt="@carru.ID_Carrusel" width="60" height="60" onclick="Salud()">
                                    }
                                    else if (carru.Accion == "Deportes")
                                    {
                                        <img class="carrusel-img" src="@imagen" alt="@carru.ID_Carrusel" width="60" height="60" onclick="Deportes()">
                                    }
                                    else if (carru.Accion == "Otros...")
                                    {
                                        <img class="carrusel-img" src="@imagen" alt="@carru.ID_Carrusel" width="60" height="60" onclick="Otros()">
                                    }
                                    else
                                    {
                                        <img class="carrusel-img" src="@imagen" alt="@carru.ID_Carrusel" width="60" height="60">
                                    }
                                }
                                else
                                {
                                    <span>No disponible</span>
                                }
                            </center>
                        </div>
                        index++;
                    }
                </div>
                <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="sr-only">Previous</span>
                </a>
                <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="sr-only">Next</span>
                </a>
            </div>
        </div>
    }
    else
    {
        <p>No hay elementos para mostrar en el carrusel.</p>
