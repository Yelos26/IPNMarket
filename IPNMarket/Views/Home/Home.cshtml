    <!-- Area de los productos -->
    <div class="area-productos">
        <br />
        <div class="productos">
            @{
                var productos = ViewData.Model as List<ProductosModel>;
                int maxProductos = 6;
                int contador = 0;

                // Mezclar la lista de productos
                var random = new Random();
                var productosAleatorios = productos.OrderBy(x => random.Next()).Take(maxProductos).ToList();
            }

            @foreach (var producto in productosAleatorios)
            {
                <div class="producto" onclick="showIframe('@producto.ID_Producto')">
                    @{
                        string imageData = "";
                        if (producto.Imagen != null && producto.Imagen.Length > 0)
                        {
                            string base64 = Convert.ToBase64String(producto.Imagen);
                            string extension = "jpg"; // Extensión predeterminada

                            // Determinar la extensión de la imagen
                            var header = producto.Imagen.AsEnumerable().Take(4).ToArray();
                            if (header.SequenceEqual(new byte[] { 137, 80, 78, 71 }))
                            {
                                extension = "png";
                            }
                            else if (producto.Imagen.AsEnumerable().Take(2).SequenceEqual(new byte[] { 255, 216 }))
                            {
                                extension = "jpeg";
                            }

                            // Crear el esquema data URI
                            imageData = $"data:image/{extension};base64,{base64}";
                        }
                    }
                    <center>
                        <h2>@producto.Nombre_Producto</h2>

                        @if (!string.IsNullOrEmpty(imageData))
                        {
                            <img src="@imageData" alt="@producto.Nombre_Producto" width="60" height="60">
                        }
                        else
                        {
                            <strong>Imagen no disponible</strong>
                        }

                        <p>@producto.Descripción</p>
                    </center>

                    @if (ViewData["Nomb"] != null)
                    {
                        <div class="contenedor">
                            @* <button onclick="showIframe('@producto.ID_Producto')">Agregar al Carrito</button> *@
                        </div>

                        <!-- CONTENEDOR DEL IFRAME -->
                        <div class="iframe-container" id="iframeContainer">
                            <iframe id="iframe"></iframe>
                        </div>

                        <script>
                            window.addEventListener('message', function (event) {
                                // Verifica si el mensaje recibido indica que se ha agregado el producto al carrito
                                if (event.data.action === 'productoAgregado') {
                                    document.body.style.overflow = 'visible';
                                    // Cierra el iframe
                                    document.getElementById('iframeContainer').style.display = 'none';
                                }
                            });

                            function showIframe(idProducto) {
                                document.getElementById('iframeContainer').style.display = 'flex';
                                document.body.style.overflow = 'hidden';
                                document.getElementById('iframe').src = "/Home/VerProducto?ID_Producto=" + idProducto;
                            }

                            // Función para ocultar el iframe y restaurar el estilo de overflow del body
                            function hideIframe() {
                                document.getElementById('iframeContainer').style.display = 'none';
                                document.body.style.overflow = 'visible';
                                document.getElementById('iframe').src = "/Home/VerProducto?ID_Producto=null";
                            }
                        </script>
                    }
                </div>

                contador++;
            }
        </div>
    </div>

